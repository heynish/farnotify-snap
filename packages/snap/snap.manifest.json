{
  "version": "1.0.1",
  "description": "Get hourly farcaster trending mints and casts on MetaMask.",
  "proposedName": "FarNotify",
  "repository": {
    "type": "git",
    "url": "https://github.com/heynish/farnotify-snap"
  },
  "source": {
    "shasum": "1XSgZD1rgAo0jIK8qQf6e1ptg6xJTz9eT1Qfn6imVZ4=",
    "location": {
      "npm": {
        "filePath": "dist/bundle.js",
        "iconPath": "images/icon.svg",
        "packageName": "farnotify-snap",
        "registry": "https://registry.npmjs.org/"
      }
    }
  },
  "initialPermissions": {
    "snap_dialog": {},
    "endowment:page-home": {},
    "endowment:lifecycle-hooks": {},
    "snap_notify": {},
    "endowment:cronjob": {
      "jobs": [
        {
          "expression": "0 * * * *",
          "request": {
            "method": "notifyCronJob"
          }
        },
        {
          "expression": "0 * * * *",
          "request": {
            "method": "garbageCollectCronJob"
          }
        }
      ]
    },
    "endowment:network-access": {}
  },
  "manifestVersion": "0.1"
}
